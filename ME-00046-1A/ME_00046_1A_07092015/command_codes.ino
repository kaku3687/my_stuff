//Command codes for I2C Message protocol, CC stands for command code
#define COMMAND_CODE_TEST                      0x01
#define COMMAND_CODE_MODULE_ID                 0x02
#define COMMAND_CODE_MODULE_VERSION            0x03
#define COMMAND_CODE_MODULE_RESET              0x04

#define COMMAND_CODE_FUNC_STATUS_CLEAR         0x22
#define COMMAND_CODE_FUNC_STATUS_CHECK         0x23

#define COMMAND_CODE_SET_SPEED                 0x32
#define COMMAND_CODE_STEP                      0x33
#define COMMAND_CODE_ONE_REV                   0x34
#define COMMAND_CODE_STEPSREV                  0x35
#define COMMAND_CODE_GETSTEPSREV               0x36
